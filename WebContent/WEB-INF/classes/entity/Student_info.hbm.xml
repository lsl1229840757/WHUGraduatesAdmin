<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="entity.Student_info" table="student_info">
		
		<id name="sid" type="java.lang.String">
			<generator class="assigned"/>
		</id>
		<!-- student_info(sid, sname, gender,political,sprov,scity,major,tel,sqq) -->
		<property name="sname" type="java.lang.String"></property>
		<property name="gender" type="java.lang.String"></property>
		<property name="political" type="java.lang.String"></property>
		<property name="sprov" type="java.lang.String"></property>
		<property name="scity" type="java.lang.String"></property>
		<property name="major" type="java.lang.String"></property>
		<property name="tel" type="java.lang.String"></property>
		<property name="sqq" type="java.lang.String"></property>
	</class>

</hibernate-mapping>
